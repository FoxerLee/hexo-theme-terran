.prev-link
    margin-top 15px
    margin-right auto
    a
        color #17141f
        &:hover
            background-color rgb(249, 248, 252)
            color #9966FF
            -webkit-transition all .2s
            -moz-transition all .2s
            -ms-transition all .2s
            -o-transition all .2s
            transition all .2s

.next-link
    margin-top 15px
    margin-left auto
    a
        color #17141f
        &:hover
            background-color rgb(249, 248, 252)
            color #9966FF
            -webkit-transition all .2s
            -moz-transition all .2s
            -ms-transition all .2s
            -o-transition all .2s
            transition all .2s
