.navbar
    max-width 1440px
    margin 0 auto

.nav-link
    font-size 15px
    &:hover
        color #9966FF !important
        font-size 15px
        background-color transparent
        text-decoration none
        cursor pointer
        -webkit-transition all .3s
        -moz-transition all .3s
        -ms-transition all .3s
        -o-transition all .3s
        transition all .3s